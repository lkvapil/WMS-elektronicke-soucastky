@startuml Konceptualni model - WMS Elektronicke soucastky
hide circle


abstract class PohybSkladu {
  - id : int
  - datum : DateTime
  - mnozstvi : int
  --
  + validovatMnozstvi()
  + ulozitZaznam()
}

class Naskladneni {
  - lokace : String
  --
  + provestNaskladneni()
  + zvysitMnozstvi()
}

class Vyskladneni {
  - cil : String
  --
  + provestVyskladneni()
  + snizitMnozstvi()
}

class Presun {
  - zdrojLokace : String
  - cilovaLokace : String
  --
  + provestPresun()
}

class SkladovaLokace {
  - kodLokace : String
  - popis : String
  --
  + pridatSoucastku()
  + odebratSoucastku()
  + zobrazObsah()
}

class Soucastka {
  - partNumber : String
  - nazev : String
  - kategorie : String
  - mnozstvi : int
  --
  + aktualizovatUdaje()
  + ziskatStav()
}

class Dodavatel {
  - nazev : String
  - zeme : String
  - adresa : String
  - email : String
  --
  + aktualizovatKontakt()
  + zobrazSoucastky()
}

' Generalizace
PohybSkladu <|-- Naskladneni
PohybSkladu <|-- Vyskladneni
PohybSkladu <|-- Presun

' Kvalifikovana vazba: Dodavatel[katalogCislo] -> 0..1 Soucastka
Dodavatel "1 [katalogCislo]" --> "0..1" Soucastka : dodava >

' Ostatni asociace
SkladovaLokace "1" -- "0..*" Soucastka : obsahuje >
PohybSkladu "0..*" --> "1" SkladovaLokace : probiha na >
PohybSkladu "0..*" --> "1" Soucastka : tyka se >

@enduml
